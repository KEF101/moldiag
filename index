        <footer>
            <p contenteditable="true"><strong>üîí RAPPORT D'ACQUISITION MOLDIAG SA üîí</strong></p>
            <p contenteditable="true"><strong>Document Strictement Confidentiel</strong></p>
            <p contenteditable="true">Analyse M&A Due Diligence Acqu√©reur | Novembre 2025</p>
        </footer>
    </div>
    
    <script>
        // ==================== VARIABLES GLOBALES ====================
        const STORAGE_KEY = 'moldiag_rapport_editable_v1';
        const ORIGINAL_CONTENT_KEY = 'moldiag_rapport_original_v1';
        const AUTO_SAVE_INTERVAL = 120000; // 2 minutes
        let hasUnsavedChanges = false;
        let autoSaveTimer = null;
        
        // ==================== SAUVEGARDE DU CONTENU ORIGINAL ====================
        function saveOriginalContent() {
            if (!localStorage.getItem(ORIGINAL_CONTENT_KEY)) {
                const originalContent = document.getElementById('editableContent').innerHTML;
                try {
                    localStorage.setItem(ORIGINAL_CONTENT_KEY, originalContent);
                    console.log('‚úÖ Contenu original sauvegard√©');
                } catch (e) {
                    console.error('‚ùå Erreur sauvegarde contenu original:', e);
                }
            }
        }
        
        // ==================== CHARGEMENT VERSION SAUVEGARD√âE ====================
        function loadSavedVersion() {
            try {
                const savedContent = localStorage.getItem(STORAGE_KEY);
                if (savedContent) {
                    document.getElementById('editableContent').innerHTML = savedContent;
                    
                    const savedDate = localStorage.getItem(STORAGE_KEY + '_date');
                    if (savedDate) {
                        updateVersionBadge('Version sauvegard√©e: ' + new Date(savedDate).toLocaleString('fr-FR'));
                    }
                    
                    showNotification('‚úÖ Version sauvegard√©e charg√©e avec succ√®s', 'success');
                    console.log('‚úÖ Version sauvegard√©e charg√©e');
                    return true;
                }
            } catch (e) {
                console.error('‚ùå Erreur chargement version sauvegard√©e:', e);
                showNotification('‚ö†Ô∏è Erreur lors du chargement de la version sauvegard√©e', 'error');
            }
            return false;
        }
        
        // ==================== SAUVEGARDE VERSION ACTUELLE ====================
        function saveCurrentVersion() {
            try {
                const content = document.getElementById('editableContent').innerHTML;
                
                // Validation contenu non vide
                if (!content || content.trim().length < 100) {
                    showNotification('‚ö†Ô∏è Contenu trop court pour √™tre sauvegard√©', 'error');
                    return false;
                }
                
                // Sauvegarde dans localStorage
                localStorage.setItem(STORAGE_KEY, content);
                localStorage.setItem(STORAGE_KEY + '_date', new Date().toISOString());
                
                // Mise √† jour UI
                hasUnsavedChanges = false;
                updateEditStatus(false);
                updateVersionBadge('Version sauvegard√©e: ' + new Date().toLocaleString('fr-FR'));
                showNotification('üíæ Modifications sauvegard√©es avec succ√®s !', 'success');
                
                console.log('‚úÖ Version sauvegard√©e avec succ√®s');
                return true;
                
            } catch (e) {
                console.error('‚ùå Erreur sauvegarde:', e);
                
                // Gestion localStorage plein
                if (e.name === 'QuotaExceededError') {
                    showNotification('‚ùå Espace de stockage insuffisant. Veuillez t√©l√©charger votre version.', 'error');
                } else {
                    showNotification('‚ùå Erreur lors de la sauvegarde', 'error');
                }
                return false;
            }
        }
        
        // ==================== RESTAURATION VERSION ORIGINALE ====================
        function restoreOriginalVersion() {
            try {
                const originalContent = localStorage.getItem(ORIGINAL_CONTENT_KEY);
                if (originalContent) {
                    document.getElementById('editableContent').innerHTML = originalContent;
                    
                    // Suppression version sauvegard√©e
                    localStorage.removeItem(STORAGE_KEY);
                    localStorage.removeItem(STORAGE_KEY + '_date');
                    
                    hasUnsavedChanges = false;
                    updateEditStatus(false);
                    updateVersionBadge('üìù Version originale');
                    showNotification('üîÑ Version originale restaur√©e avec succ√®s !', 'success');
                    
                    closeModal();
                    console.log('‚úÖ Version originale restaur√©e');
                    
                    // R√©initialiser les zones √©ditables
                    makeContentEditable();
                } else {
                    showNotification('‚ùå Version originale introuvable', 'error');
                }
            } catch (e) {
                console.error('‚ùå Erreur restauration:', e);
                showNotification('‚ùå Erreur lors de la restauration', 'error');
            }
        }
        
        // ==================== T√âL√âCHARGEMENT HTML ====================
        function downloadAsHTML() {
            try {
                // R√©cup√©ration du HTML complet avec les modifications
                const htmlContent = document.documentElement.outerHTML;
                
                // Cr√©ation blob
                const blob = new Blob([htmlContent], { type: 'text/html;charset=utf-8' });
                
                // Cr√©ation lien t√©l√©chargement
                const link = document.createElement('a');
                link.href = URL.createObjectURL(blob);
                link.download = 'MOLDIAG_Rapport_Edite_' + new Date().toISOString().split('T')[0] + '.html';
                
                // D√©clenchement t√©l√©chargement
                document.body.appendChild(link);
                link.click();
                document.body.removeChild(link);
                
                showNotification('üì• Fichier HTML t√©l√©charg√© avec succ√®s !', 'success');
                console.log('‚úÖ HTML t√©l√©charg√©');
                
            } catch (e) {
                console.error('‚ùå Erreur t√©l√©chargement:', e);
                showNotification('‚ùå Erreur lors du t√©l√©chargement', 'error');
            }
        }
        
        // ==================== D√âTECTION MODIFICATIONS ====================
        function trackChanges() {
            hasUnsavedChanges = true;
            updateEditStatus(true);
            
            // Reset auto-save timer
            clearTimeout(autoSaveTimer);
            autoSaveTimer = setTimeout(autoSave, AUTO_SAVE_INTERVAL);
        }
        
        // ==================== AUTO-SAUVEGARDE ====================
        function autoSave() {
            if (hasUnsavedChanges) {
                saveCurrentVersion();
                console.log('üîÑ Auto-sauvegarde effectu√©e');
            }
        }
        
        // ==================== MISE √Ä JOUR STATUT √âDITION ====================
        function updateEditStatus(unsaved) {
            const statusElement = document.getElementById('editStatus');
            if (unsaved) {
                statusElement.textContent = '‚ö†Ô∏è Modifications non sauvegard√©es';
                statusElement.classList.add('unsaved');
            } else {
                statusElement.textContent = '‚úÖ Tout sauvegard√©';
                statusElement.classList.remove('unsaved');
            }
        }
        
        // ==================== MISE √Ä JOUR BADGE VERSION ====================
        function updateVersionBadge(text) {
            document.getElementById('versionBadge').textContent = text;
        }
        
        // ==================== NOTIFICATION ====================
        function showNotification(message, type = 'success') {
            const notification = document.getElementById('saveNotification');
            notification.textContent = message;
            notification.className = 'save-notification show';
            
            if (type === 'error') {
                notification.classList.add('error');
            }
            
            setTimeout(() => {
                notification.classList.remove('show');
            }, 3000);
        }
        
        // ==================== MODAL CONFIRMATION ====================
        function showRestoreModal() {
            document.getElementById('confirmModal').classList.add('show');
        }
        
        function closeModal() {
            document.getElementById('confirmModal').classList.remove('show');
        }
        
        function confirmRestore() {
            restoreOriginalVersion();
        }
        
        // ==================== RENDRE CONTENU √âDITABLE ====================
        function makeContentEditable() {
            // Rendre tous les √©l√©ments de contenu √©ditables
            const editableElements = document.querySelectorAll('#editableContent [contenteditable="true"]');
            
            editableElements.forEach(element => {
                element.addEventListener('input', trackChanges);
                element.addEventListener('paste', (e) => {
                    // Nettoyage du texte coll√© (retrait formatage)
                    e.preventDefault();
                    const text = (e.clipboardData || window.clipboardData).getData('text');
                    document.execCommand('insertText', false, text);
                });
            });
            
            console.log(`‚úÖ ${editableElements.length} √©l√©ments rendus √©ditables`);
        }
        
        // ==================== NAVIGATION SMOOTH ====================
        function setupSmoothScroll() {
            document.querySelectorAll('a[href^="#"]').forEach(anchor => {
                anchor.addEventListener('click', function (e) {
                    e.preventDefault();
                    const target = document.querySelector(this.getAttribute('href'));
                    if (target) {
                        target.scrollIntoView({ behavior: 'smooth', block: 'start' });
                    }
                });
            });
        }
        
        // ==================== HIGHLIGHT NAVIGATION ====================
        function setupNavHighlight() {
            window.addEventListener('scroll', () => {
                const sections = document.querySelectorAll('section');
                const navLinks = document.querySelectorAll('nav a');
                let current = '';
                
                sections.forEach(section => {
                    const sectionTop = section.offsetTop;
                    if (pageYOffset >= sectionTop - 200) {
                        current = section.getAttribute('id');
                    }
                });
                
                navLinks.forEach(link => {
                    link.style.backgroundColor = '';
                    if (link.getAttribute('href') === '#' + current) {
                        link.style.backgroundColor = '#1e3c72';
                    }
                });
            });
        }
        
        // ==================== PR√âVENIR PERTE MODIFICATIONS ====================
        window.addEventListener('beforeunload', (e) => {
            if (hasUnsavedChanges) {
                e.preventDefault();
                e.returnValue = 'Vous avez des modifications non sauvegard√©es. Voulez-vous vraiment quitter ?';
            }
        });
        
        // ==================== RACCOURCIS CLAVIER ====================
        document.addEventListener('keydown', (e) => {
            // Ctrl+S ou Cmd+S pour sauvegarder
            if ((e.ctrlKey || e.metaKey) && e.key === 's') {
                e.preventDefault();
                saveCurrentVersion();
            }
            
            // Ctrl+P ou Cmd+P pour imprimer
            if ((e.ctrlKey || e.metaKey) && e.key === 'p') {
                e.preventDefault();
                window.print();
            }
        });
        
        // ==================== INITIALISATION AU CHARGEMENT ====================
        document.addEventListener('DOMContentLoaded', () => {
            console.log('üöÄ Initialisation application √©ditable MOLDIAG...');
            
            // 1. Sauvegarder contenu original
            saveOriginalContent();
            
            // 2. Charger version sauvegard√©e si existe
            const hasLoadedSaved = loadSavedVersion();
            if (!hasLoadedSaved) {
                updateVersionBadge('üìù Version originale');
            }
            
            // 3. Rendre contenu √©ditable
            makeContentEditable();
            
            // 4. Setup navigation
            setupSmoothScroll();
            setupNavHighlight();
            
            // 5. Setup auto-save
            autoSaveTimer = setTimeout(autoSave, AUTO_SAVE_INTERVAL);
            
            console.log('‚úÖ Application pr√™te !');
            console.log('üí° Utilisez Ctrl+S pour sauvegarder rapidement');
        });
        
        // ==================== GESTION ERREURS GLOBALES ====================
        window.addEventListener('error', (e) => {
            console.error('‚ùå Erreur globale:', e.error);
            showNotification('‚ùå Une erreur est survenue', 'error');
        });
        
    </script>
</body>
</html>
